<h2>Edit Profile</h2>
<br>
<%= form_for :profilecreate_path, url: profilecreate_path do |f| %>
  <div class="field">
    <h4>User name</h4>
    <%= f.text_field :cart ,class:"form-control", style:"width: 20%;"%>
  </div>
  <br>
  <div class="field">
    <h4>Profile Image</h4>
    <%= f.file_field :avatar %>
  </div>
  <br>
  <div class="actions">

    <%= f.submit class: "my-submit-button"  %>
  </div>
<% end %>
